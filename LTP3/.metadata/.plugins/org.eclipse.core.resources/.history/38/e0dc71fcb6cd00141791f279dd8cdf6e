import java.util.Random;
public class Exercicio5 {

	public static void main(String[] args) {
		Random number = new Random();
		int aux = 0;
		int auxDois;
		for (int n=0; n<=5; n++) {
			do {
				auxDois = number.nextInt(6);
				if(auxDois == aux) {
					aux = number.nextInt(6);
					auxDois = aux;
				} else {
				System.out.println(aux);
				}
			}while(auxDois != aux);
				
		}

	}

}
