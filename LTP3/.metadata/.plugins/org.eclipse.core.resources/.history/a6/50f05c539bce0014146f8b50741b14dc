import utilitarios.Console;

public class Lista1 {

	public static void main(String[] args) {
		int valor = Console.readInt("---Menu---\n"
				+ "1-Exercicio\n"
				+ "2-Exercicio\n"
				+ "3-Exercicio\n"
				+ "4=Exercicio\n"
				+ "5-Exercicio\n");
		
		switch (valor){
		
		case 1: 
			exercicio1();
			break;
		case 2:
			exercicio2();
			break;
		case 3:
			exercicio3();
			break;
		case 4:
			exercicio4();
			break;
		case 5:
			exercicio5();
			break;
		default:
			System.out.println("Opção inválida!");
		}
		

	}

	private static void exercicio1() {
		for (int i=0; i<=9; i++) {
			System.out.println((int) Math.ceil(Math.random() *10));
		}
		
	}

	private static void exercicio2() {
		for (int i=1; i<=10; i++) {
			System.out.println((int) Math.ceil(Math.random() *6) + 1);
		}
		
	}

	private static void exercicio3() {
		// TODO Auto-generated method stub
		
	}

	private static void exercicio4() {
		// TODO Auto-generated method stub
		
	}

	private static void exercicio5() {
		// TODO Auto-generated method stub
		
	}

}
